{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "semi": [
      2,
      "always"
    ]
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
